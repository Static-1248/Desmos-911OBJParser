	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "Launch Program",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}\\src\\index.ts",
			"preLaunchTask": "tsc: build - tsconfig.json",
			"outFiles": [
				"${workspaceFolder}/**/*.js"
			],
			"console": "integratedTerminal",
			"runtimeVersion": "20",
			"args": ["./Aircraft-simplified.obj", "-v", "V_0", "-f", "F_0", "-o", "expressions.json"]
		}
	]
}